#include<stdio.h>
#include<stdlib.h>
int main()
{
	int a, b, c, d, e, f,g;
	scanf("%d %d", &c, &f);
	c -= 1;
	e = 0;
	for (a = f;a > 0;a--) 
	{
		e++;
		c++;
		g = c;
		for (b = 0;b < a-1;b++)
		{
			printf(" ");
		}
		for (d = 1;d <= 2 * e - 1;d++)
		{
			if (d <= e)
			{
				printf("%c", g-d+1);
			
			}
			else
			{
				
				printf("%c",g-2*e+1+d);
			}
		}
		printf("\n");
	}
	system("pause");
	return 0;
}
